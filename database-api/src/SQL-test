/*
here lies all the SQL code used in test 
of the server functions copy and paste
as needed.
name of db is nick_test
*/

CREATE DATABASE nick_test;
CREATE TABLE nick_test.users(
fname varchar(150) NOT NULL,
lname varchar(150) NOT NULL,
userID int UNIQUE AUTO_INCREMENT,
primary key(userID)
);
CREATE TABLE nick_test.eventTable(
eventID int UNIQUE auto_increment,
eventTime time NOT NULL,
eventDate date NOT NULL,
eventName varchar(150) NOT NULL,
primary key(eventID)
);
CREATE TABLE nick_test.eventUserTable(
userID int NOT NULL,
eventID int NOT NULL,
foreign key(eventID) REFERENCES nick_test.eventTable(eventID),
foreign key(userID) REFERENCES nick_test.users(userID),
roles varchar(9)
);

INSERT INTO nick_test.users (fname, lname)
values ("Jeff", "Bezos"),
		("Connor", "Bendover"),
		("Nick", "Dickinson"),
        ("Spencer", "Kleen-star");
 
INSERT INTO nick_test.eventTable(eventID, eventTime, 
eventDate, eventName)
values(123, "12:00:00", "2021/04/10", "Lunch"),
	  (124, "13:00:00", "2021/04/10", "Meeting");
      
INSERT INTO nick_test.eventUserTable(eventID, userID, roles)
values(123, 1, "Leader"),
	  (123, 2, "Moderator"),
      (124, 3, "Leader"),
      (124, 4, "Candidate");